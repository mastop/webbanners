</head>
<body class="wbbody wbbody_{%if color is defined%}{{color}}{%else%}cinza{%endif%}{%if is_granted('ROLE_USER') %} logged{%endif%}">
{# Header #}
{% block header %}
{%if is_granted('ROLE_USER') %}      
<header id="hdC">
    <div class="container"> 
        <div id="Table_header">
            <div id="header-02_">
                <a class="brand" href="#">
                    <div class="span3">
                        <a id="logo" href="{{ path('_home') }}" title="{{ mastop_param('system.site.name') }} - Home">{{ mastop_param('system.site.name') }} - {{ mastop_param('system.site.slogan') }}</a>
                    </div>
                </a>
            </div>
            <div id="header-03_">
                <div class="navbar floatR">
                    <div class="nav-collapse">
                        <ul class="nav pull-right">
                            {{mastop_menu('system-head', null, null, 0, 'list', {'class':'nav head'})}}
                        </ul>
                    </div>
                </div>
                {%if is_granted('ROLE_USER') %}
                <div class="alert alert-message login floatR">
                    <a href="{{path('user_dashboard_index')}}" class="button small blue">Minha conta</a>
                </div>
                {%endif%}
            </div>
            <div id="header-06_">
                <div class="navbar">
                    <div class="nav-collapse">
                        <ul class="nav">
                            {{mastop_menu('system-main', null, null, 0, 'list', {'class':'nav menu'})}}
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
</header>
{% else %}
<header id="hd">
    <div class="container"> 
        <div id="Table_header">
            <div id="header-02_">
                <a class="brand" href="#">
                    <div class="span3">
                        <a id="logo" href="{{ path('_home') }}" title="{{ mastop_param('system.site.name') }} - Home">{{ mastop_param('system.site.name') }} - {{ mastop_param('system.site.slogan') }}</a>
                    </div>
                </a>
            </div>
            <div id="header-03_">
                <div class="navbar floatR">
                    <div class="nav-collapse">
                        <ul class="nav pull-right">
                            {{mastop_menu('system-head', null, null, 0, 'list', {'class':'nav head'})}}
                        </ul>
                    </div>
                </div>
                {%if is_granted('ROLE_USER') %}
                <div class="alert alert-message login floatR">
                    <span>Olá, <strong>{{ app.user.name|default(app.user.username) }}</strong>! <small>(não é você? <a href="{{path('_logout')}}">clique aqui</a>)</small></span> <a href="{{path('user_dashboard_index')}}" class="button small blue">Minha conta</a>
                </div>
                {%endif%}
            </div>
            <div id="header-04_">
            </div>
            <div id="header-05_">
            </div>
            <div id="header-06_">
                <div class="navbar">
                    <div class="nav-collapse">
                        <ul class="nav">
                            {{mastop_menu('system-main', null, null, 0, 'list', {'class':'nav menu'})}}
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
</header>
{%endif%}
{%endblock%}
{# /Header #}
{# Body #}
{% block body %}
<div id="body" role="foot" class="container">
    {# Mensagens do Sistema #}
    {% if app.session.hasFlash('error') %}
    <div class="alert alert-error">
            <a class="close" data-dismiss="alert" href="#">&times;</a>
            {{ app.session.flash('error')|trans|raw }}
    </div>
    {% endif %}
    {% if app.session.hasFlash('ok') %}
    <div class="alert alert-success">
            <a class="close" data-dismiss="alert" href="#">&times;</a>
            {{ app.session.flash('ok')|trans|raw }}
    </div>
    {% endif %}
    {% if app.session.hasFlash('notice') %}
    <div class="alert alert-info">
            <a class="close" data-dismiss="alert" href="#">&times;</a>
            {{ app.session.flash('notice')|trans|raw }}
    </div>
    {% endif %}
    {% if app.session.hasFlash('warning') %}
    <div class="alert alert-warning">
            <a class="close" data-dismiss="alert" href="#">&times;</a>
            {{ app.session.flash('warning')|trans|raw }}
    </div>
    {% endif %}
    {# /Mensagens do Sistema #}
    {# Conteúdo #}
    <div class="container content">
        <div class="span11">
        {% block content null %}
        </div>
    </div>
    {# /Conteúdo #}
</div>
{%endblock%}
{# /Body #}
{# Facebook div #}
<div id="fb-root" class="hidden"></div>
{#/Facebook div #}
<footer id="ft">
    <div class="container">
        <div id="Table_footer">
            <div id="footer-01_">
            </div>
            <div id="footer-02_">
                <div id="ftLogo">
                    <a href="{{ path('_home') }}" title="Homepage">{{ mastop_param('system.site.name') }} - {{ mastop_param('system.site.slogan') }}</a>
                </div>
            </div>
            <div id="footer-03_">
                {{mastop_menu('system-foot', null, null, 0, 'BannerCoreBundle:Menu:footer.html.twig', {'id':'navFoot'})}}
            </div>
            <div id="footer-04_">
            </div>
            <div id="footer-05_">
            </div>
            <div id="footer-06_">
                <div id="copyright">
                    {% block footer %} {{ mastop_param('system.site.footer') }} {% endblock%}
                </div>
            </div>
            <div id="footer-07_">
                <div id="poweredBy">
                        <a href="http://www.mastop.com.br" title="Produzido por Mastop Internet Development">Mastop Devenvolvimento e Consultoria Web</a>
                </div>
            </div>
        </div>
    </div>
</footer>


  <!-- JavaScript at the bottom for fast page loading -->

