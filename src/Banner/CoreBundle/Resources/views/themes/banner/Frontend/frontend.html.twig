    <!-- Le javascript -->
    {% javascripts 'banner/Frontend/twitter-bootstrap/js/bootstrap-tooltip.js' 'banner/Frontend/twitter-bootstrap/js/bootstrap-*.js' filter='yui_js' output='static/system/js/bootstrap.js' %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}

    <script>
    var facebookRedir="{{ redirURL|default(mastop_current_url()) }}";
    var facebookAppId="{{ mastop_param('user.all.faceappid') }}";
    var facebookUrl="{{url("user_user_facebook")}}";
    </script>
    
    {% javascripts '@BannerUserBundle/Resources/public/js/Frontend/facebookScripts.js' '@BannerCoreBundle/Resources/public/js/chosen/chosen.jquery.min.js' '@BannerCoreBundle/Resources/public/js/jquery.color.js' '@BannerCoreBundle/Resources/public/js/jquery.pulse.js' '@BannerCoreBundle/Resources/public/js/jquery.cookie.js' '@BannerCoreBundle/Resources/public/js/core.js' filter='yui_js' output='static/system/js/packed.js' %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
  
    {% javascripts '@BannerCoreBundle/Resources/public/js/facebox/facebox.js' filter='yui_js' output='static/system/js/facebox.js' %}
        <script src="{{ asset_url }}" type="text/javascript"></script>
    {% endjavascripts %}
        
