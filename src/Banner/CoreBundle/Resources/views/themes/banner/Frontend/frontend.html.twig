    {% javascripts '@BannerUserBundle/Resources/public/js/Frontend/facebookScripts.js' '@BannerCoreBundle/Resources/public/js/chosen/chosen.jquery.min.js' '@BannerCoreBundle/Resources/public/js/jquery.color.js' '@BannerCoreBundle/Resources/public/js/jquery.pulse.js' '@BannerCoreBundle/Resources/public/js/jquery.cookie.js' '@BannerCoreBundle/Resources/public/js/core.js' filter='yui_js' output='static/system/js/packed.js' %}
