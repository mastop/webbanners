  {% javascripts '@BannerCoreBundle/Resources/public/js/libs/modernizr-2.0.6.min.js' filter='yui_js' output='static/system/js/modernizr.js' %}
    <script src="{{ asset_url }}"></script>
  {% endjavascripts %}
