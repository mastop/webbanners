<head>
  <meta charset="utf-8">
  <title>{% block title title|default(mastop_param('system.site.name'))|trans %} - {%if app.request.attributes.get('_route') == '_home'%}{{mastop_param('system.site.slogan')}}{%else%}{{mastop_param('system.site.name')}}{%endif%}</title>
  <meta name="description" content="{% block metaDescription mastop_param('system.seo.description') %}">
  <meta name="author" content="Mastop Internet Development">
  <meta name="viewport" content="width=device-width,initial-scale=1">
  
  <!-- CSS Tema -->
  {% stylesheets 'banner/Frontend/css/system/*.css' filter='?yui_css' output='themes/banner/css/packed.css' %}
   <link href="{{ asset_url }}" type="text/css" rel="stylesheet" />
  {% endstylesheets %}
  
  <!-- CSS Tema -->
  {% stylesheets '@BannerCoreBundle/Resources/public/css/*.css' filter='?yui_css' output='themes/banner/css/banner.css' %}
   <link href="{{ asset_url }}" type="text/css" rel="stylesheet" />
  {% endstylesheets %}
   
  <!-- Bootstrap LESS -->
  {% stylesheets filter='less,?yui_css' '@BannerCoreBundle/Resources/views/themes/banner/Frontend/twitter-bootstrap/less/bootstrap.less' '@BannerCoreBundle/Resources/views/themes/banner/Frontend/twitter-bootstrap/less/responsive.less' output='themes/banner/twitter-bootstrap/css/bootstrap.css' %}
    <link href="{{ asset_url }}" rel="stylesheet" media="screen" />
  {% endstylesheets %}

  <!-- Facebox CSS -->
  {% stylesheets '@BannerCoreBundle/Resources/public/js/facebox/facebox.css' filter='?yui_css' output='themes/banner/facebox/css/facebox.css' %}
    <link href="{{ asset_url }}" rel="stylesheet" media="screen"/>
  {% endstylesheets %}
  <!-- CSS System -->
  {% block stylesheets null%}

  <!-- More ideas for your <head> here: h5bp.com/d/head-Tips -->

